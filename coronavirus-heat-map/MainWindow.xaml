<Window x:Class="coronavirus_heat_map.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:coronavirus_heat_map"
        mc:Ignorable="d"
        Title="coronavirus-heat-map"
        Height="500"
        Width="800"
        Icon="images/icon.png"
        WindowStyle="None"
        AllowsTransparency="True"
        Background="#121212"
        ResizeMode="NoResize">

    <!-- making custom Window Chrome Bar -->
    <WindowChrome.WindowChrome>
        <WindowChrome CaptionHeight="35" />
    </WindowChrome.WindowChrome>

    <Grid>
        <Grid VerticalAlignment="Top"
              Background="#121212">
            <StackPanel VerticalAlignment="Center"
                        HorizontalAlignment="Left"
                        Height="35"
                        WindowChrome.IsHitTestVisibleInChrome="True"
                        Margin="20,5">
                <TextBlock Foreground="White"
                           Name="Title"
                           FontSize="12"
                           Margin="5, 5, 5, 5">
                    𝙘𝙤𝙧𝙤𝙣𝙖𝙫𝙞𝙧𝙪𝙨-𝙝𝙚𝙖𝙩-𝙢𝙖𝙥
                </TextBlock>
            </StackPanel>
            <StackPanel Height="35"
                        WindowChrome.IsHitTestVisibleInChrome="True"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Right"
                        Orientation="Horizontal">
                <Button Width="35"
                        Content="-"
                        Name="MinimizeButton"
                        Foreground="White"
                        Background="#121212"
                        BorderThickness="0"
                        FontSize="16" />
                <Button Width="35"
                        Content="&lt;/&gt;"
                        Name="Github"
                        Foreground="White"
                        Background="#121212"
                        BorderThickness="0"
                        FontSize="16" />
                <Button Width="35"
                        Content="?"
                        Name="CDC"
                        Foreground="White"
                        Background="#121212"
                        BorderThickness="0"
                        FontSize="16" />

                <!-- Maximize button <Button Width="35" Content="+" Name="MaximizeButton" Foreground="Gray" Background="#121212" BorderThickness="0" FontSize="16" />-->
                <Button Width="35"
                        Content="x"
                        Name="CloseButton"
                        Foreground="White"
                        Background="#121212"
                        BorderThickness="0"
                        FontSize="16" />
            </StackPanel>
        </Grid>

        <!-- Left SideBar -->
        <Grid Name="leftSideBar"
              Margin="20, 45, 550, 20">
            <StackPanel Background="#292929"
                            Height="105"
                            VerticalAlignment="Top">
                <TextBlock Margin="10,0,10,10"
                               Name="tested"
                               Foreground="#B2B2B2"
                               FontSize="20">Tested</TextBlock >
                <TextBlock Margin="10,-15,10,10"
                               Name="numTested"
                               Foreground="white"
                               FontSize="50">--</TextBlock >
                <TextBlock Margin="20,-15,10,10"
                               Name="percentTestedIncrease"
                                HorizontalAlignment="Right"
                               Foreground="#B2B2B2"
                               FontSize="12"></TextBlock >
            </StackPanel>

            <StackPanel Background="#292929"
                            Height="105"
                            Margin="0,110,0,0"
                            VerticalAlignment="Top">
                <TextBlock Margin="10,0,10,10"
                               Name="positive"
                               Foreground="#B2B2B2"
                               FontSize="20">Positive</TextBlock >
                <TextBlock Margin="10,-15,10,10"
                               Name="numPositive"
                               Foreground="white"
                               FontSize="50">--</TextBlock >
                <TextBlock Margin="20,-15,10,10"
                               Name="percentPositiveIncrease"
                                HorizontalAlignment="Right"
                               Foreground="#B2B2B2"
                               FontSize="12"></TextBlock >
            </StackPanel>
            <StackPanel Background="#292929"
                            Height="105"
                            Margin="0,220,0,0"
                            VerticalAlignment="Top">
                <TextBlock Margin="10,0,10,10"
                               Name="deaths"
                               Foreground="#B2B2B2"
                               FontSize="20">Deaths</TextBlock >
                <TextBlock Margin="10,-15,10,10"
                               Name="numDeaths"
                               Foreground="white"
                               FontSize="50">--</TextBlock >
                <TextBlock Margin="20,-15,10,10"
                               Name="percentDeathsIncrease"
                                HorizontalAlignment="Right"
                               Foreground="#B2B2B2"
                               FontSize="12"></TextBlock >
            </StackPanel>

            <StackPanel Background="#292929"
                            Height="105"
                            Margin="0,330,0,0"
                            VerticalAlignment="Top">
                <TextBlock Foreground="#B2B2B2"
                           FontSize="20"
                           HorizontalAlignment="Left"
                           Margin="10,0,10,10" Panel.ZIndex="1">
                    Totals
                </TextBlock>

                <!-- bar graph container, WxH: 230x85 -->
                <Grid Background="#292929"
                      Margin="0,-34,0,0"
                            Height="85"
                      Name="barGraph">

                    <!-- actual graph WxH: 220x75 -->
                    <Grid Background="#292929"
                          Height="75"
                          Width="220">

                        <!-- last sixMonths -->
                        <StackPanel Background="#292929"
                              Width="35"
                              Name="month1">
                            <TextBlock Foreground="White"
                                       FontSize="7"
                                       Name="month1Name">
                            </TextBlock>
                        </StackPanel>

                        <StackPanel Background="#292929"
                              Width="35"
                              Name="month2">
                            <TextBlock Foreground="White"
                                       FontSize="7"
                                       Name="month2Name">
                            </TextBlock>
                        </StackPanel>

                        <StackPanel Background="#292929"
                              Width="35"
                              Name="month3">
                            <TextBlock Foreground="White"
                                       FontSize="7"
                                       Name="month3Name">
                            </TextBlock>
                        </StackPanel>

                        <StackPanel Background="#292929"
                              Width="35"
                              Name="month4">
                            <TextBlock Foreground="White"
                                       FontSize="7"
                                       Name="month4Name">
                            </TextBlock>
                        </StackPanel>

                        <StackPanel Background="#292929"
                              Width="35"
                              Name="month5">
                            <TextBlock Foreground="White"
                                       FontSize="7"
                                       Name="month5Name">
                            </TextBlock>
                        </StackPanel>

                        <StackPanel Background="#292929"
                              Width="35"
                              Name="month6">
                            <TextBlock Foreground="White"
                                       FontSize="7"
                                       Name="month6Name">
                            </TextBlock>
                        </StackPanel>

                    </Grid>

                </Grid>

                <!-- options below bar graph -->
                <StackPanel Background="#292929" 
                            Orientation="Horizontal"
                            Margin="0,0,0,0"
                            Height="20">
                    <TextBlock Margin="5, 0, 0, 5"
                               Width="72"
                               Name="testedBar"
                               Foreground="#B2B2B2"
                               PreviewMouseDown="barGraphText"
                               Tag="tested"
                               FontSize="12">Tested</TextBlock >
                    <TextBlock Margin="14, 0, 0, 5"
                               Name="positiveBar"
                               Width="72"
                               Foreground="#B2B2B2"
                               PreviewMouseDown="barGraphText"
                               Tag="positive"
                               FontSize="12">Positive</TextBlock >
                    <TextBlock Margin="24, 0, 0, 5"
                               Name="deathsBar"
                               Width="72"
                               Foreground="#B2B2B2"
                               PreviewMouseDown="barGraphText"
                               Tag="deaths"
                               FontSize="12">Deaths</TextBlock >

                </StackPanel>
            </StackPanel>
        </Grid>

        <!-- Right Side -->
        <Grid Name="rightSideBar"
              Margin="280, 45, 20, 20"
              Background="#292929">

            <!-- Map container -->
            <StackPanel Background="#292929"
                        Height="400"
                        Margin="0, -40, 0, 0">

                <Viewbox Width="500" Height="400"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        >
                    <Canvas Name="MAP" Width="927.400" Height="587.225" IsEnabled="True">
                        <Canvas.Resources>
                            <Style TargetType="Path">
                                <Setter Property="Stroke" Value="White"/>
                                <!-- was White -->
                                <Setter Property="Fill" Value="#ffb1b1b1"/>
                                <!-- #ffb1b1b1 -->
                                <Style.Triggers>
                                    <EventTrigger RoutedEvent="Mouse.MouseEnter">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                                                    <LinearColorKeyFrame KeyTime="0:0:0.3"
                                                 Value="Red"/>
                                                </ColorAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>

                                    <EventTrigger RoutedEvent="Mouse.MouseLeave">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                                                    <LinearColorKeyFrame KeyTime="0:0:0.3"
                                                 Value="#ffb1b1b1"/>
                                                </ColorAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>

                                    <EventTrigger RoutedEvent="Mouse.MouseDown">
                                        <BeginStoryboard>
                                            <Storyboard>
                                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)">
                                                    <LinearColorKeyFrame KeyTime="0:0:0.3"
                                                 Value="LimeGreen"/>
                                                </ColorAnimationUsingKeyFrames>
                                            </Storyboard>
                                        </BeginStoryboard>
                                    </EventTrigger>

                                </Style.Triggers>
                            </Style>
                        </Canvas.Resources>

                        <Canvas>

                            <Canvas>

